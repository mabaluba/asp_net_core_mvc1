@model IEnumerable<Family>
@{
    ViewData["Title"] = "Family";
}

<h1>Index - Family</h1>

<div class="container p-3">
    <div class="raw pt-4">
        <div class="col-6">
            <h2 class=" text-primary">Моя семья</h2>
        </div>
        <div class="col-6">Create new item</div>
    </div>

    @if (Model.Any())
     {
         @foreach (var item in Model)
          {
              <div>@item.Id - @item.Name</div>
          }
     }
     else
     {
          <p>It is empty</p>
     }
    
</div>